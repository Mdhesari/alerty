{
  "name": "rezaamini-ir/alerty",
  "license": "MIT",
  "description": "Alertify will monitor your queries and alert you!",
  "authors": [
    {
      "name": "Reza Amini",
      "email": "rezaaminiroyal@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^6.0|^7.0|^8.0|^9.0",
    "laravel/framework": "^6.0|^7.0|^8.0"
  },
  "require-dev": {
    "orchestra/testbench": "~3.7|^4.0|^5.0|^6.0"
  },
  "autoload": {
    "psr-4": {
      "Alerty\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AlertyTest\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Alerty\\AlertyServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
